#https://superuser.com/questions/465472/how-to-connect-bluetooth-headset-via-command-line-on-ubuntu-12-04
jblId="B8:D5:0B:C7:B8:89" #run bluetoothctl and then scan to get id
xbt="10:4F:A8:33:18:70"
alias connectJbl="echo \"connect $jblId\" | bluetoothctl"
alias disconnectJbl="echo \"disconnect $jblId\" | bluetoothctl"
alias connectXbt="echo \"connect $xbt\" | bluetoothctl"
alias disconnectXbt="echo \"disconnect $xbt\" | bluetoothctl"
